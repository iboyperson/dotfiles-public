{
    
    "files.associations": {
        "*.el": "lisp"
    },
    "bracketPairColorizer.timeOut": 0,
    "cSpell.diagnosticLevel": "Hint",
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "gitignore",
        "go",
        "gradle",
        "handlebars",
        "html",
        "ini",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "lisp",
        "makefile",
        "markdown",
        "php",
        "plaintext",
        "properties",
        "pub",
        "python",
        "restructuredtext",
        "ruby",
        "rust",
        "scss",
        "shellscript",
        "swift",
        "tex",
        "text",
        "toml",
        "typescript",
        "typescriptreact",
        "viml",
        "xml",
        "yaml"
    ],
    "cSpell.language": "en",
    "code-runner.enableAppInsights": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.minimap.enabled": false,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "githubPullRequests.telemetry.enabled": false,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.advanced.telemetry.enabled": false,
    "latex-workshop.message.update.show": false,
    "python.formatting.provider": "black",
    "python.jediMemoryLimit": -1,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.external.linuxExec": "konsole",
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.scrollback": 100000,
    "window.zoomLevel": 0,
    "workbench.startupEditor": "newUntitledFile",
    "files.autoSave": "afterDelay",
    "java.saveActions.organizeImports": true,
    "java.errors.incompleteClasspath.severity": "ignore",
    "files.eol": "\n",
    "breadcrumbs.enabled": true,
    "go.autocompleteUnimportedPackages": true,
    "go.lintTool":"golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "java.home": "/usr/lib/jvm/default",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "workbench.colorTheme": "One Dark Pro",
    "search.location": "panel",
    "zenMode.centerLayout": false,
    "java.configuration.checkProjectSettingsExclusions": false,
    "window.menuBarVisibility": "visible",
    "code-runner.clearPreviousOutput": true,
	"code-runner.executorMap": {
        "c": "mkdir -p $workspaceRoot/build && clang $fileName -o $workspaceRoot/build/$fileNameWithoutExt && $workspaceRoot/build/$fileNameWithoutExt",
        "cpp": "mkdir -p $workspaceRoot/build && clang++ $fileName -o $workspaceRoot/build/$fileNameWithoutExt && $workspaceRoot/build/$fileNameWithoutExt",
        "objective-c": "mkdir -p $workspaceRoot/build && gcc -framework Cocoa $fileName -o $workspaceRoot/build/$fileNameWithoutExt && $workspaceRoot/build/$fileNameWithoutExt",
		"rust": "cd $dir && cargo run $fileName",
    },
    "workbench.iconTheme": "vscode-icons",
    "git.autofetch": true,
    "explorer.confirmDragAndDrop": false,
    "rust.clippy_preference": "on",
    "omnisharp.enableEditorConfigSupport": true,
    "extensions.autoUpdate": false,
    "vsicons.dontShowNewVersionMessage": true,
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.format.settings.profile": "GoogleStyle",
    "workbench.activityBar.visible": false,
    "git.defaultCloneDirectory": "",
    "terminal.integrated.rendererType": "experimentalWebgl",
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "todo-tree.tree.flat": true,
    "todo-tree.tree.grouped": true,
    "todo-tree.highlights.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "todo-tree.general.tags": [
		"TODO",
		"FIXME"
    ],
    "better-comments.tags": [
		{
			"tag": "!",
			"color": "#E06C75",
			"strikethrough": false,
			"backgroundColor": "transparent"
		},
		{
			"tag": "?",
			"color": "#61AFEF",
			"strikethrough": false,
			"backgroundColor": "transparent"
		},
		{
			"tag": "//",
			"color": "#474747",
			"strikethrough": true,
			"backgroundColor": "transparent"
		},
		{
			"tag": "todo",
			"color": "#E5C07B",
			"strikethrough": false,
			"backgroundColor": "transparent"
		},
		{
			"tag": "*",
			"color": "#98C379",
			"strikethrough": false,
			"backgroundColor": "transparent"
		}
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "task.slowProviderWarning": [
        "gradle"
    ],
}